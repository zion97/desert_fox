function MobSpr(_stand1, _stand2, _walk, _run) constructor
{
	spr_stand1	= _stand1;
	spr_stand2	= _stand2;
	spr_walk	= _walk;
	spr_run		= _run;
	
}

function MobInfo(_hp, _walk_spd, _run_spd) constructor
{
	index_hp	= hp;
}